<?xml version="1.0" encoding="UTF-8"?>
<Basic xmlns="http://hl7.org/fhir" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://hl7.org/fhir ../../schema/fhir-single.xsd">
  <!-- This example demonstrates the KnowledgeModule profile used to represent a knowledge module -->
  <id value="knowledgemodule-module-example"/>
  <meta>
	  <profile value="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-cqif-knowledgemodule"/>
  </meta>
  <!-- Module Identifier -->
  <extension url="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-moduleIdentifier">
	  <valueIdentifier>
		  <value value="guideline-appropriate-ordering-module"/>
	  </valueIdentifier>
  </extension>
  <!-- Type -->
  <extension url="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-type">
	  <valueCode value="module"/>
  </extension>
  <!-- Topic -->
  <!-- Keyword -->
  <!-- Patient Data Requirement -->
  <extension url="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-data">
	  <extension url="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-data#type">
		  <valueCode value="Patient"/>
	  </extension>
	  <extension url="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-data#profile">
		  <valueReference reference="http://hl7.org/fhir/StructureDefinition/patient-gao-gao-patient"/>
	  </extension>
  </extension>
  <!-- Order Data Requirement -->
  <extension url="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-data">
	  <extension url="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-data#type">
		  <valueCode value="Order"/>
	  </extension>
	  <extension url="http://hl7.org/fhir/StructureDefinition/knowledgemodule-cqif-data#profile">
		  <valueReference reference="http://hl7.org/fhir/StructureDefinition/order-gao-gao-order"/>
	  </extension>
  </extension>
</Basic>